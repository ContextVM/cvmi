{
  "name": "cvmi",
  "version": "0.1.9",
  "description": "ContextVM Interface",
  "type": "module",
  "bin": {
    "cvmi": "./bin/cli.mjs",
    "contextvm": "./bin/cli.mjs"
  },
  "files": [
    "dist",
    "bin",
    "README.md"
  ],
  "scripts": {
    "build": "obuild",
    "dev": "node src/cli.ts",
    "exec:test": "node scripts/execute-tests.ts",
    "prepublishOnly": "npm run build",
    "format": "prettier --write 'src/**/*.ts' 'scripts/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts' 'scripts/**/*.ts'",
    "prepare": "husky",
    "test": "vitest src",
    "type-check": "tsc --noEmit",
    "release": "pnpm run build && npm publish",
    "publish:snapshot": "npm version prerelease --preid=snapshot --no-git-tag-version && npm publish --tag snapshot"
  },
  "lint-staged": {
    "src/**/*.ts": "prettier --write",
    "scripts/**/*.ts": "prettier --write",
    "tests/**/*.ts": "prettier --write"
  },
  "keywords": [
    "cli",
    "agent-skills",
    "skills",
    "ai-agents",
    "amp",
    "antigravity",
    "claude-code",
    "moltbot",
    "cline",
    "codebuddy",
    "codex",
    "command-code",
    "continue",
    "crush",
    "cursor",
    "droid",
    "gemini-cli",
    "github-copilot",
    "goose",
    "junie",
    "kilo",
    "kimi-cli",
    "kiro-cli",
    "kode",
    "mcpjam",
    "mux",
    "opencode",
    "openclaude",
    "openhands",
    "pi",
    "qoder",
    "qwen-code",
    "replit",
    "roo",
    "trae",
    "trae-cn",
    "windsurf",
    "zencoder",
    "neovate",
    "pochi"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vercel-labs/skills.git"
  },
  "homepage": "https://github.com/vercel-labs/skills#readme",
  "bugs": {
    "url": "https://github.com/vercel-labs/skills/issues"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@contextvm/sdk": "^0.3.2",
    "@modelcontextprotocol/sdk": "^1.25.3",
    "nostr-tools": "^2.23.0",
    "xdg-basedir": "^5.1.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@clack/prompts": "^0.11.0",
    "@types/bun": "latest",
    "@types/node": "^22.19.8",
    "gray-matter": "^4.0.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "obuild": "^0.4.22",
    "picocolors": "^1.1.1",
    "prettier": "^3.8.1",
    "simple-git": "^3.30.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.17.1"
}
